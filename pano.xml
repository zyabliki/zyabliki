    <hotspots height="20" wordwrap="1" width="180">
      <label height="20" border="1" bordercolor="0x000000" textalpha="1" backgroundalpha="1" wordwrap="1" textcolor="0x000000" borderalpha="1" enabled="1" borderradius="1" background="1" backgroundcolor="0xffffff" width="180"/>
