      <label backgroundcolor="0xffffff" borderalpha="1" border="1" backgroundalpha="1" background="1" borderradius="1" height="20" bordercolor="0x000000" width="180" enabled="1" textalpha="1" wordwrap="1" textcolor="0x000000"/>
