      <label bordercolor="0x000000" borderradius="1" background="1" backgroundcolor="0xffffff" backgroundalpha="1" textalpha="1" textcolor="0x000000" enabled="1" height="20" borderalpha="1" width="180" border="1" wordwrap="1"/>
