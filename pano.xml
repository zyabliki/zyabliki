      <label border="1" textcolor="0x000000" height="20" borderradius="1" textalpha="1" background="1" bordercolor="0x000000" backgroundalpha="1" borderalpha="1" enabled="1" wordwrap="1" width="180" backgroundcolor="0xffffff"/>
